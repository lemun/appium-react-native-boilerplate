{
    "compilerOptions": {
    "resolveJsonModule": true,
        "sourceMap": false,
        "target": "es2020",
        "module": "NodeNext",
        "moduleResolution": "Node16",
        "removeComments": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": [
            "node",
            "@wdio/types",
            "@wdio/globals/types",
            "@wdio/mocha-framework",
            "expect-webdriverio",
        ]
    },
    "include": [
        "./src/**/*.ts",
        "wdio.ios.conf.ts",
        "wdio.android.conf.ts",
        "wdio.conf.ts"
    ]
}