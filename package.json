{
  "name": "appium-react-native-boilerplate",
  "version": "1.0.0",
  "author": "Shai Sinai",
  "description": "Appium + WebDriverIO boilerplate for cross-platform React Native E2E testing",
  "homepage": "https://github.com/lemun/appium-react-native-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemun/appium-react-native-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/lemun/appium-react-native-boilerplate/issues"
  },
  "license": "ISC",
  "keywords": [],
  "type": "module",
  "scripts": {
    "ios": "npx wdio run wdio.ios.conf.ts",
    "android": "npx wdio run wdio.android.conf.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "validate": "npm run lint && npm run format:check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@wdio/appium-service": "^9.18.0",
    "@wdio/cli": "^9.18.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.18.0",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "appium": "^2.19.0",
    "appium-uiautomator2-driver": "^4.2.8",
    "appium-xcuitest-driver": "^9.10.4",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-wdio": "^9.16.2",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}